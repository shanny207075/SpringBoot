<?xml version="1.0" encoding="UTF-8"?> 
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ext="http://www.ibm.com/xmlns/dde/schema/annotation/ext"> 
    <xsd:complexType name="com.ibm.ws.app.manager.webappcfg"> 
        <xsd:annotation> 
            <xsd:documentation>定義 Web 應用程式的內容。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Web 應用程式</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader"> 
            </xsd:element> 
            <xsd:element name="application-bnd" type="com.ibm.ws.javaee.dd.appbnd"> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="location" use="required" type="location"> 
            <xsd:annotation> 
                <xsd:documentation>應用程式的位置，以伺服器層次應用程式目錄的絕對路徑或相對路徑表示。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>位置</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>應用程式的名稱。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>名稱</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="autoStart" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指出伺服器是否自動啟動應用程式。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>自動啟動</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="suppressUncoveredHttpMethodWarning" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>在應用程式部署期間用來暫停未涵蓋的 HTTP 方法警告訊息的選項。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>暫停未涵蓋的 HTTP 方法警告</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="contextRoot" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>應用程式的環境定義根目錄。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>應用程式環境定義根目錄</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
