                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="loginModuleRef" use="optional" default="hashtable,userNameAndPassword,certificate,token" type="pidListType"> 
            <xsd:annotation> 
                <xsd:documentation>JAAS 登入模組 ID 的參照。</xsd:documentation> 
