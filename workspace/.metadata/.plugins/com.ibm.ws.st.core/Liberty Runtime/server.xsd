    <xsd:complexType name="com.ibm.ws.http.log.access"> 
        <xsd:annotation> 
            <xsd:documentation>HTTP 存取日誌包含所有入埠 HTTP 用戶端要求的記錄。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>HTTP 存取記載</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="enabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>啟用存取記載。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>已啟用</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="filePath" use="optional" default="${server.output.dir}/logs/http_access.log" type="fileLocation"> 
            <xsd:annotation> 
                <xsd:documentation>存取日誌檔的目錄路徑和名稱。指定目錄路徑時，可以使用標準變數替代項目，例如 ${server.output.dir}。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>日誌檔路徑</ext:label> 
                    <ext:unique>accessLogFilePath</ext:unique> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="logFormat" use="optional" default="%h %u %{t}W &quot;%r&quot; %s %b" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定記載用戶端存取資訊時要使用的日誌格式。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>格式</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxFileSize" use="optional" default="20"> 
            <xsd:annotation> 
                <xsd:documentation>日誌檔在輪替之前的大小上限 (MB)；0 值表示無限制。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>日誌檔大小上限</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="maxFiles" use="optional" default="2"> 
            <xsd:annotation> 
                <xsd:documentation>在移除最舊檔案之前，將保留的日誌檔數目上限；0 值表示無限制。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>日誌檔數目上限</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.http.log.access-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.http.log.access"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.http.proxyredirect"> 
        <xsd:annotation> 
            <xsd:documentation>配置埠重新導向。將 HTTP 要求從非安全埠（例如 80）重新導向至啟用 SSL 的安全埠（例如 443）時，使用「HTTP Proxy 重新導向」。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>HTTP Proxy 重新導向</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="httpPort" use="required"> 
            <xsd:annotation> 
                <xsd:documentation>重新導向時的來源（非安全）埠。此埠上送入的 HTTP 要求會重新導向至指定的 HTTPS 埠。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>HTTP 埠</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="1"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="65535"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="httpsPort" use="required"> 
            <xsd:annotation> 
                <xsd:documentation>重新導向時的目標（安全）埠。使用 HTTP 埠之送入的 HTTP 要求會重新導向至這個埠。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>HTTPS 埠</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="1"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="65535"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="enabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>這個屬性決定伺服器是否應重新導向這個配置元素中指定的埠。預設值是 true。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>已啟用</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="host" use="optional" default="*" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>重新導向此埠時，要使用的主機名稱。送入的要求所指定的主機名稱必須符合此值，伺服器才會重新導向 HTTP 要求。預設值是 *（所有主機）。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>主機</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.http.proxyredirect-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.http.proxyredirect"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.http"> 
        <xsd:annotation> 
            <xsd:documentation>HTTP 端點的配置內容。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>HTTP 端點</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="httpOptions" type="com.ibm.ws.http.options"> 
                <xsd:annotation> 
                    <xsd:documentation>端點的 HTTP 通訊協定選項。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>HTTP 選項</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="sslOptions" type="com.ibm.ws.sslchannel.options"> 
                <xsd:annotation> 
                    <xsd:documentation>端點的 SSL 通訊協定選項。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>SSL 選項</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="tcpOptions" type="com.ibm.ws.tcpchannel.options"> 
                <xsd:annotation> 
                    <xsd:documentation>端點的 TCP 通訊協定選項。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>TCP 選項</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="accessLogging" type="com.ibm.ws.http.log.access"> 
                <xsd:annotation> 
                    <xsd:documentation>端點的 HTTP 存取記載配置。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>HTTP 存取記載</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="enabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>切換此端點的可用性。如果是 true，分派器便會啟動這個端點來處理 HTTP 要求。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>已啟用</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="host" use="optional" default="localhost" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>用戶端用來要求資源的 IP 位址、含網域名稱字尾的網域名稱伺服器 (DNS) 主機名稱，或只是 DNS 主機名稱。使用 '*' 來代表所有可用網路介面。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>主機</ext:label> 
                    <ext:variable>defaultHostName</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="httpPort" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>用戶端 HTTP 要求所用的埠。使用 -1 可停用此埠。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>埠</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="-1"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="65535"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="httpsPort" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>受到 SSL (https) 保護的用戶端 HTTP 要求所用的埠。使用 -1 可停用此埠。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>安全埠</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="-1"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="65535"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="onError" use="optional" default="WARN"> 
            <xsd:annotation> 
                <xsd:documentation>無法啟動端點之後要採取的動作。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>錯誤時</ext:label> 
                    <ext:variable>onError</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="WARN"> 
                                <xsd:annotation> 
                                    <xsd:documentation>伺服器在引起配置錯誤時，將會發出警告和錯誤訊息。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="FAIL"> 
                                <xsd:annotation> 
                                    <xsd:documentation>伺服器將在第一次出現錯誤時發出警告或錯誤訊息，然後停止伺服器。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="IGNORE"> 
                                <xsd:annotation> 
                                    <xsd:documentation>伺服器在引起配置錯誤時，將不會發出任何警告和錯誤訊息。 </xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="httpOptionsRef" use="optional" default="defaultHttpOptions" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>端點的 HTTP 通訊協定選項。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>HTTP 選項參照</ext:label> 
                    <ext:reference>httpOptions</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslOptionsRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>端點的 SSL 通訊協定選項。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>SSL 選項參照</ext:label> 
                    <ext:reference>sslOptions</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="tcpOptionsRef" use="optional" default="defaultTCPOptions" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>端點的 TCP 通訊協定選項。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>TCP 選項參照</ext:label> 
                    <ext:reference>tcpOptions</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="accessLoggingRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>端點的 HTTP 存取記載配置。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>HTTP 存取記載參照</ext:label> 
                    <ext:reference>httpAccessLogging</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.http-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.http"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.http.options"> 
        <xsd:annotation> 
            <xsd:documentation>HTTP 通訊協定配置。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>HTTP 選項</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="keepAliveEnabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>啟用持續性連線（HTTP 保持作用中）。如果是 true，連線會保持作用中，供多個循序要求和回應重複使用。如果是 false，則會送出回應之後關閉連線。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用持續性連線</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxKeepAliveRequests" use="optional" default="100"> 
            <xsd:annotation> 
                <xsd:documentation>啟用持續性連線時，單一 HTTP 連線所允許的持續性要求數目上限。值 -1 表示無限制。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>每個連線的持續性要求數目上限</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="-1"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="persistTimeout" use="optional" default="30s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>允許 Socket 在要求之間保持閒置的時間量。只有在啟用持續性連線時，這項設定才適用。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>持續性連線逾時值</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="readTimeout" use="optional" default="60s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>在進行第一次讀取之後，在 Socket 上等待讀取要求完成的時間量。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>讀取逾時</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="writeTimeout" use="optional" default="60s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>在 Socket 上等待傳輸每部分回應資料的時間量。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>寫入逾時</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="removeServerHeader" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>將伺服器實作資訊從 HTTP 標頭移除，同時停用預設的 Liberty 設定檔歡迎使用頁面。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>移除伺服器標頭</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.http.options-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.http.options"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.http.virtualhost"> 
        <xsd:annotation> 
            <xsd:documentation>虛擬主機提供邏輯分組，可將 Web 應用程式配置成特定主機名稱。預設虛擬主機 (default_host) 適用於大部分簡易的配置。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>虛擬主機</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="hostAlias" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>使用 host:port 語法來建立主機和埠與此虛擬主機的關聯性。指定的主機可以是 IP 位址、含網域名稱字尾的網域名稱伺服器 (DNS) 主機名稱、DNS 主機名稱，或 * 表示以萬用字元比對所有主機名稱。請注意，IPv6 位址必須以 [] 括住。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>虛擬主機別名</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="allowFromEndpoint" type="com.ibm.ws.http-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>指定一或多個 HTTP 端點的 ID，以限制虛擬主機只接受來自指定端點的入埠資料流量。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>容許來自端點</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="enabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>啟用這個虛擬主機。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>已啟用</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowFromEndpointRef" use="optional" type="pidListType"> 
            <xsd:annotation> 
                <xsd:documentation>指定一或多個 HTTP 端點的 ID，以限制虛擬主機只接受來自指定端點的入埠資料流量。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>容許來自端點</ext:label> 
                    <ext:reference>httpEndpoint</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.http.virtualhost-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.http.virtualhost"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.transport.iiop.internal.ORBWrapper"> 
        <xsd:annotation> 
            <xsd:documentation>伺服器或用戶端 ORB 的配置。若為用戶端 ORB，請指定 nameService 屬性；若為伺服器 ORB，請指定一或多個 iiopEndpoint 參照。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Object Request Broker (ORB)</ext:label> 
            </xsd:appinfo> 
