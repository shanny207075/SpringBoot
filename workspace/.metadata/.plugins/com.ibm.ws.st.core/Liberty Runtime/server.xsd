    <xsd:complexType name="com.ibm.ws.security.authentication.internal.jaas.jaasLoginModuleConfig"> 
        <xsd:annotation> 
            <xsd:documentation>JAAS 配置中的登入模組。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>JAAS 登入模組</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="library" type="com.ibm.ws.classloading.sharedlibrary"> 
                <xsd:annotation> 
                    <xsd:documentation>共用程式庫配置 ID 的參照。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>共用程式庫</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="options" type="com.ibm.ws.security.authentication.internal.jaas.jaasLoginModuleConfig.options"> 
                <xsd:annotation> 
                    <xsd:documentation>「JAAS 登入」模組選項的集合</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>JAAS 登入模組選項</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="className" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>JAAS 登入模組類別的完整套件名稱。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>類別名稱</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="controlFlag" use="optional" default="REQUIRED"> 
            <xsd:annotation> 
                <xsd:documentation>登入模組的控制旗標。有效值為 REQUIRED、REQUISITE、SUFFICIENT 和 OPTIONAL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>登入模組控制旗標</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="REQUIRED"> 
                                <xsd:annotation> 
                                    <xsd:documentation>根據 JAAS 規格，這個 LoginModule 是「必要的」。必須要有 LoginModule 才能成功。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="REQUISITE"> 
                                <xsd:annotation> 
                                    <xsd:documentation>根據 JAAS 規格，這個 LoginModule 是 REQUISITE。必須要有 LoginModule 才能成功。如果鑑別失敗，則不會呼叫其他 LoginModule，同時控制權會回到呼叫端。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="SUFFICIENT"> 
                                <xsd:annotation> 
                                    <xsd:documentation>根據 JAAS 規格，這個 LoginModule 是「足夠的」。不需要有 LoginModule 就能成功。如果鑑別成功，則不會呼叫其他 LoginModule，同時控制權會回到呼叫端。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="OPTIONAL"> 
                                <xsd:annotation> 
                                    <xsd:documentation>根據 JAAS 規格，這個 LoginModule 是「選用的」。不需要有 LoginModule 就能成功。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="libraryRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>共用程式庫配置 ID 的參照。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>共用程式庫</ext:label> 
                    <ext:reference>library</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.authentication.internal.jaas.jaasLoginModuleConfig-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.authentication.internal.jaas.jaasLoginModuleConfig"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
