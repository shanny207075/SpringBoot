                    <xsd:appinfo> 
                        <ext:label>資料夾</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="file" type="com.ibm.ws.classloading.sharedlibrary.file-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>參照的檔案 ID</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>檔案</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>管理者的共用程式庫名稱</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>名稱</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="description" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>管理者的共用程式庫說明</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>說明</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="filesetRef" use="optional" type="pidListType"> 
            <xsd:annotation> 
                <xsd:documentation>參照的檔案集 ID</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>檔案集參照</ext:label> 
                    <ext:reference>fileset</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="apiTypeVisibility" use="optional" default="spec,ibm-api,api" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>這個程式庫的類別載入器將能夠看到的 API 套件類型，方式為下列項目之任何組合的逗點區隔清單：spec、ibm-api、api、third-party。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>容許的 API 類型</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.classloading.sharedlibrary-factory"> 
