    <xsd:complexType name="com.ibm.ws.concurrent.managedThreadFactory"> 
        <xsd:annotation> 
            <xsd:documentation>受管理執行緒 Factory</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>受管理執行緒 Factory</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="contextService" minOccurs="0" maxOccurs="1" type="com.ibm.ws.context.service"> 
                <xsd:annotation> 
                    <xsd:documentation>配置如何將環境定義延伸到執行緒</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>執行緒環境定義延伸</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="contextServiceRef" use="optional" default="DefaultContextService" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>配置如何將環境定義延伸到執行緒</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>環境定義延伸參照</ext:label> 
                    <ext:reference>contextService</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="createDaemonThreads" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>配置受管理執行緒 Factory 所建立的執行緒是否應該是常駐程式執行緒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>建立常駐程式執行緒</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="defaultPriority" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>受管理執行緒 Factory 所建立之執行緒的預設優先順序。如果沒有指定，會使用正在建立之執行緒的優先順序。優先順序不能超過受管理執行緒 Factory 的優先順序上限，在這種情況下，會改用優先順序上限。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>執行緒的預設優先順序</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="1"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="10"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="jndiName" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>JNDI 名稱</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>JNDI 名稱</ext:label> 
                    <ext:unique>jndiName</ext:unique> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxPriority" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>受管理執行緒 Factory 所建立之執行緒的優先順序上限。</xsd:documentation> 
