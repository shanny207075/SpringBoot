            <xsd:annotation> 
                <xsd:documentation>將使用者的名稱對映到 LDAP 項目的 LDAP 過濾器。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>使用者 ID 對映</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupIdMap" use="optional" default="*:cn" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>將群組的名稱對映到 LDAP 項目的 LDAP 過濾器。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>群組 ID 對映</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupMemberIdMap" use="optional" default="groupOfNames:member;groupOfUniqueNames:uniqueMember" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>用來識別具備群組成員資格之使用者的 LDAP 過濾器。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>群組成員 ID 對映</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.netscape-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.registry.ldap.internal.filters.netscape"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.wim.adapter.ldap.membershipAttribute"> 
        <xsd:annotation> 
            <xsd:documentation>LDAP 成員資格屬性的配置。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>LDAP 成員資格屬性</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>成員資格屬性的名稱。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>成員資格屬性名稱</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="scope" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>成員資格屬性的範圍。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>範圍</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.wim.adapter.ldap.membershipAttribute-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.wim.adapter.ldap.membershipAttribute"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.logging"> 
        <xsd:annotation> 
            <xsd:documentation>控制日誌和追蹤訊息的擷取及輸出。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>記載</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="maxFileSize" use="optional" default="20"> 
            <xsd:annotation> 
                <xsd:documentation>日誌檔在輪替之前的大小上限 (MB)；0 值表示無限制。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>日誌檔大小上限</ext:label> 
                    <ext:variable>com.ibm.ws.logging.max.file.size</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="maxFiles" use="optional" default="2"> 
            <xsd:annotation> 
                <xsd:documentation>在移除最舊檔案之前，將保留的日誌檔數目上限；0 值表示無限制。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>日誌檔數目上限</ext:label> 
                    <ext:variable>com.ibm.ws.logging.max.files</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="logDirectory" use="optional" default="${server.output.dir}/logs" type="dirLocation"> 
            <xsd:annotation> 
                <xsd:documentation>日誌檔的目錄位置。預設值是 ${server.output.dir}/logs。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>日誌目錄</ext:label> 
                    <ext:variable>com.ibm.ws.logging.log.directory</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="messageFileName" use="optional" default="messages.log" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>訊息輸出將寫入其中的檔案名稱（相對於已配置的日誌目錄）。預設值是 messages.log。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>訊息檔名稱</ext:label> 
                    <ext:variable>com.ibm.ws.logging.message.file.name</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="traceFileName" use="optional" default="trace.log" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>追蹤輸出將寫入其中的檔案名稱（相對於已配置的日誌目錄）。預設值是 trace.log。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>追蹤檔名稱</ext:label> 
                    <ext:variable>com.ibm.ws.logging.trace.file.name</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="traceSpecification" use="optional" default="*=info" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>符合追蹤規格文法且指定各種追蹤元件起始狀態的追蹤規格。空值可接受且視為「停用所有追蹤」。任何未指定的元件都會起始設定為預設狀態 *=info。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>追蹤規格</ext:label> 
                    <ext:variable>com.ibm.ws.logging.trace.specification</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="hideMessage" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>配置要在 console.log 和 message.log 檔中隱藏的訊息清單（以逗點區隔）。如果將訊息配置成隱藏，會將它們重新導向至 trace.log 檔。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>要隱藏的訊息</ext:label> 
                    <ext:variable>com.ibm.ws.logging.hideMessage</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="traceFormat" use="optional" default="ENHANCED"> 
            <xsd:annotation> 
                <xsd:documentation>這種格式用於追蹤日誌。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>追蹤格式</ext:label> 
                    <ext:variable>com.ibm.ws.logging.trace.format</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="BASIC"> 
                                <xsd:annotation> 
                                    <xsd:documentation>使用基本追蹤格式。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="ENHANCED"> 
                                <xsd:annotation> 
                                    <xsd:documentation>使用加強的基本追蹤格式。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="ADVANCED"> 
                                <xsd:annotation> 
                                    <xsd:documentation>使用進階追蹤格式。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="consoleLogLevel" use="optional" default="AUDIT"> 
            <xsd:annotation> 
                <xsd:documentation>此記載層次用來過濾寫入系統串流的訊息。預設值是審核。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>主控台記載層次</ext:label> 
                    <ext:variable>com.ibm.ws.logging.console.log.level</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="INFO"> 
                                <xsd:annotation> 
                                    <xsd:documentation>資訊、審核和警告訊息將寫入系統輸出串流。錯誤訊息將寫入系統錯誤串流。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="AUDIT"> 
                                <xsd:annotation> 
                                    <xsd:documentation>審核和警告訊息將寫入系統輸出串流。錯誤訊息將寫入系統錯誤串流。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="WARNING"> 
                                <xsd:annotation> 
                                    <xsd:documentation>警告訊息將寫入系統輸出串流。錯誤訊息將寫入系統錯誤串流。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="ERROR"> 
                                <xsd:annotation> 
                                    <xsd:documentation>錯誤訊息將寫入系統錯誤串流。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="OFF"> 
                                <xsd:annotation> 
                                    <xsd:documentation>沒有任何伺服器輸出會寫入系統串流。只有 JVM 輸出會寫入系統串流。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="copySystemStreams" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>若為 true，則會將 System.out 寫入到系統輸出串流，將 System.err 寫入到錯誤串流。若為 false，則 System.out 和 System.err 將寫入到所配置的日誌，例如 messages.log 或 trace.log，但不會寫入到系統串流。預設值為 true。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>將 System.out 和 System.err 複製到系統串流</ext:label> 
                    <ext:variable>com.ibm.ws.logging.copy.system.streams</ext:variable> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="suppressSensitiveTrace" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>當追蹤非類型化資料時，例如通過網路連線接收位元組，伺服器追蹤可以顯現機密資料。若是如此，請防止在日誌和追蹤檔中機密資訊的潛在顯現。預設值是 false。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>暫停機密追蹤</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.token.ltpa.LTPAConfiguration"> 
        <xsd:annotation> 
            <xsd:documentation>「小型認證機構 (LTPA)」記號配置。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>LTPA 記號</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="keysFileName" use="optional" default="${server.output.dir}/resources/security/ltpa.keys" type="fileLocation"> 
            <xsd:annotation> 
                <xsd:documentation>包含記號金鑰之檔案的路徑。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>LTPA 記號金鑰檔</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="keysPassword" use="optional" default="{xor}CDo9Hgw=" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>記號金鑰的密碼。其值可以用明碼或編碼形式儲存。建議您將密碼編碼，請搭配使用 securityUtility 工具與編碼選項。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>LTPA 記號金鑰密碼</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="expiration" use="optional" default="120m" type="minuteDuration"> 
            <xsd:annotation> 
                <xsd:documentation>記號到期之前的時間量（分鐘）。. 指定正整數加上時間單位，時間單位可以是小時 (h) 或分鐘 (m)。例如，指定 30 分鐘為 30m。您可以在單一項目中包括多個值。例如，1h30m 相等於 90 分鐘。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>LTPA 記號有效期限</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="monitorInterval" use="optional" default="0ms" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>伺服器檢查 LTPA 記號金鑰檔更新項目的頻率。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m)、秒 (s) 或毫秒 (ms)。例如，指定 500 毫秒為 500ms。您可以在單一項目中包括多個值。例如，1s500ms 相等於 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>LTPA 記號金鑰檔輪詢率</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.ejbcontainer.runtime"> 
        <xsd:annotation> 
            <xsd:documentation>定義 EJB 儲存器的行為。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>EJB 儲存器</ext:label> 
                <ext:groupDecl id="Advanced"/> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="asynchronous" type="com.ibm.ws.ejbcontainer.asynchronous.runtime"> 
                <xsd:annotation> 
                    <xsd:documentation>定義 EJB 非同步方法的行為。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>EJB 非同步方法</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="timerService" type="com.ibm.ws.ejbcontainer.timer.runtime"> 
                <xsd:annotation> 
                    <xsd:documentation>定義 EJB 計時器服務的行為。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>EJB 計時器服務</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="poolCleanupInterval" use="optional" default="30s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>移除未用 Bean 實例的間隔時間。這項設定僅適用於 Stateless Session 和訊息驅動 Bean。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>儲存區清理間隔</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cacheSize" use="optional" default="2053"> 
            <xsd:annotation> 
                <xsd:documentation>應快取到記憶體中的 Stateful Session Bean 實例數目。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>快取記憶體大小</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="1"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="cacheCleanupInterval" use="optional" default="3s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>當超過大小時，取消未用的 Stateful Session Bean 實例的間隔時間。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>快取清理間隔</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="startEJBsAtAppStart" use="optional" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定起始設定 EJB 類型的時機。如果將這個內容設為 true，則 EJB 類型會在應用程式第一次啟動時起始設定。如果將這個內容設為 false，則 EJB 類型會在應用程式第一次使用 EJB 類型時起始設定。如果未設定這個內容，就會根據 ibm-ejb-jar-ext.xml 檔中的 start-at-app-start 屬性，逐一判定 Bean 的行為。這項設定不適用於訊息驅動 Bean 或啟動單態 Bean。訊息驅動 Bean 和啟動單態 Bean 一律會在應用程式啟動時起始設定。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>在應用程式啟動時啟動 EJB</ext:label> 
                    <ext:group id="Advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.monitor.internal.MonitoringFrameworkExtender"> 
        <xsd:annotation> 
            <xsd:documentation>監視特性的配置，包括啟用的傳統 PMI、FineGrained 和未來任何的配置更新。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>監視器</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="enableTraditionalPMI" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>用來啟用或停用報告的「傳統 PMI」方式的內容。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用傳統 PMI</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="filter" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>可讓使用者根據群組名稱（例如 WebContainer、JVM、ThreadPool、Session、ConnectionPool 等），來啟用/停用監視器。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>已過濾（已監視）群組</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.concurrent.persistent.executor"> 
        <xsd:annotation> 
            <xsd:documentation>排定並執行持續性作業。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>排定的持續性執行程式</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="contextService" minOccurs="0" maxOccurs="1" type="com.ibm.ws.context.service"> 
                <xsd:annotation> 
                    <xsd:documentation>配置如何擷取環境定義並延伸到執行緒。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>執行緒環境定義延伸</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="contextServiceRef" use="optional" default="DefaultContextService" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>配置如何擷取環境定義並延伸到執行緒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>執行緒環境定義延伸參照</ext:label> 
                    <ext:reference>contextService</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="enableTaskExecution" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>決定這個實例是否可執行作業。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用作業執行</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="initialPollDelay" use="optional" default="0" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>在這個實例可輪詢持續儲存庫中要執行的作業之前，要先等待的時間。值 -1 會延遲輪詢，直到以程式設計方式來開始為止。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m)、秒 (s) 或毫秒 (ms)。例如，指定 500 毫秒為 500ms。您可以在單一項目中包括多個值。例如，1s500ms 相等於 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>起始輪詢延遲</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="pollInterval" use="optional" default="-1" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>輪詢要執行的作業的間隔時間。值 -1 表示在起始輪詢之後即停用所有的輪詢。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m)、秒 (s) 或毫秒 (ms)。例如，指定 500 毫秒為 500ms。您可以在單一項目中包括多個值。例如，1s500ms 相等於 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>輪詢間隔</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="pollSize" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>在輪詢持續儲存庫中要執行的作業時，所要尋找的作業項目數上限。如果未指定，則表示沒有限制。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>輪詢大小</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="1"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="retryInterval" use="optional" default="1m" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>失敗作業在第二次和後續連續重試之間必須經過的時間量。不論這個屬性的值為何，第一次重試會立即發生。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m)、秒 (s) 或毫秒 (ms)。例如，指定 500 毫秒為 500ms。您可以在單一項目中包括多個值。例如，1s500ms 相等於 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>重試間隔</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="retryLimit" use="optional" default="10"> 
            <xsd:annotation> 
                <xsd:documentation>失敗或回復的作業的連續重試次數限制，一旦超過這個次數，會將作業視為永久失敗，且不再嘗試進一步的重試。值 -1 容許無限制重試。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>重試限制</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:short"> 
                            <xsd:minInclusive value="-1"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="10000"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="taskStoreRef" use="optional" default="defaultDatabaseStore" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>排定作業的持續儲存庫。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>持續性作業儲存庫參照</ext:label> 
                    <ext:reference>databaseStore</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.concurrent.persistent.executor-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.concurrent.persistent.executor"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.request.timing.RequestTimingService"> 
        <xsd:annotation> 
            <xsd:documentation>提供慢速或當掉要求的警告和診斷資訊。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>要求計時</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="slowRequestThreshold" use="optional" default="10s" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>要求在被視為慢速之前可持續執行的時間。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m)、秒 (s) 或毫秒 (ms)。例如，指定 500 毫秒為 500ms。您可以在單一項目中包括多個值。例如，1s500ms 相等於 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>慢速要求臨界值</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="hungRequestThreshold" use="optional" default="10m" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>要求在被視為當掉之前可持續執行的時間。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m)、秒 (s) 或毫秒 (ms)。例如，指定 500 毫秒為 500ms。您可以在單一項目中包括多個值。例如，1s500ms 相等於 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>當掉要求臨界值</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sampleRate" use="optional" default="1"> 
            <xsd:annotation> 
                <xsd:documentation>為了追蹤慢速要求而應該進行取樣的比率。如果要從每 n 個要求中取樣一個，請將 sampleRate 設為 n。如果要取樣所有要求，請將 sampleRate 設為 1。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>取樣率</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="1"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="includeContextInfo" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指出是否在日誌輸出中包含環境定義詳細資訊。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>包含環境定義資訊</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.management.filetransfer"> 
        <xsd:annotation> 
            <xsd:documentation>這個元素包含用來控制遠端連線所顯現之檔案存取層次的構件。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>遠端檔案存取</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="readDir" type="dirLocation"> 
                <xsd:annotation> 
                    <xsd:documentation>遠端用戶端可以讀取的目錄。可以有多個 readDir 元素，各元素分別代表可能指向變數或絕對路徑的單一目錄。預設值是 ${wlp.install.dir}、${wlp.user.dir} 和 ${server.output.dir}</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>讀取權目錄</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="writeDir" type="dirLocation"> 
                <xsd:annotation> 
                    <xsd:documentation>遠端用戶端可以讀取和寫入的目錄。可以有多個 writeDir 元素，各元素分別代表可能指向變數或絕對路徑的單一目錄。預設值是一組空白的目錄。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>寫入權目錄</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.quickStartSecurity"> 
        <xsd:annotation> 
            <xsd:documentation>簡式管理安全配置。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>快速入門安全</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="userName" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>定義為快速入門安全配置之一部分的單一使用者。這位使用者獲授予「管理者」角色。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>使用者名稱</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userPassword" use="required" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>定義為快速入門安全配置之一部分的單一使用者密碼。建議您將此密碼編碼。如果要這麼做，請搭配使用 securityUtility 工具與編碼選項。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>使用者密碼</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.management.security.role.administrator"> 
        <xsd:annotation> 
            <xsd:documentation>獲指派伺服器管理者角色的使用者及/或群組集合。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>管理者角色</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="user" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>獲指派角色的使用者。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>使用者名稱</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="group" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>獲指派角色的群組。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>群組名稱</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.context"> 
        <xsd:annotation> 
            <xsd:documentation>當指定此項時，會將工作起始器的安全環境定義延伸到工作單元。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>安全環境定義</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.context-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.context"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.session"> 
        <xsd:annotation> 
            <xsd:documentation>HTTP 階段作業管理的配置。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>HTTP 階段作業</ext:label> 
                <ext:extraProperties/> 
                <ext:groupDecl id="session.tracking.mechanism" label="階段作業追蹤機制">指定 HTTP 階段作業管理的機制。</ext:groupDecl> 
                <ext:groupDecl id="invalidation" label="失效">控制如何清除未用的階段作業。</ext:groupDecl> 
                <ext:groupDecl id="cookies" label="Cookie">控制如何使用 Cookie。</ext:groupDecl> 
                <ext:groupDecl id="url.rewriting" label="URL 重新編寫">控制如何使用 URL 重新編寫。</ext:groupDecl> 
                <ext:groupDecl id="security" label="安全">控制如何維護階段作業安全。</ext:groupDecl> 
                <ext:groupDecl id="failover" label="失效接手">控制如何取消和啟動伺服器之間的階段作業。</ext:groupDecl> 
                <ext:groupDecl id="advanced" label="進階">這些選項的預設值適用於大部分的環境。</ext:groupDecl> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="maxInMemorySessionCount" use="optional" default="1000" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>針對每個 Web 模組在記憶體內維護的階段作業數目上限。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>記憶體內階段作業數目上限</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowOverflow" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>容許記憶體內的階段作業數目可以超出「記憶體內階段作業數目上限」內容的值。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>容許階段作業溢位</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslTrackingEnabled" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定階段作業追蹤利用 Secure Sockets Layer (SSL) 資訊來作為階段作業 ID。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用 SSL ID 追蹤</ext:label> 
                    <ext:group id="session.tracking.mechanism"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cookiesEnabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定階段作業追蹤利用 Cookie 來傳送階段作業 ID。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用 Cookie</ext:label> 
                    <ext:group id="session.tracking.mechanism"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="urlRewritingEnabled" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定階段作業管理機能利用重新編寫的 URL 來傳送階段作業 ID。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用 URL 重新編寫</ext:label> 
                    <ext:group id="session.tracking.mechanism"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="invalidationTimeout" use="optional" default="30m" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>階段作業在失效之前可保持未使用的時間量。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>階段作業逾時值</ext:label> 
                    <ext:group id="invalidation"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="reaperPollInterval" use="optional" default="-1" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>移除無效階段作業之程序的起動間隔（以秒為單位）。下限值為 30 秒。如果輸入小於下限的值，會自動判定並使用適當的值。這個值會根據階段作業逾時值來置換預設安裝值，其介於 30 到 360 秒之間。由於預設階段作業逾時值是 30 分鐘，因此收割者間隔通常是介於 2 到 3 分鐘之間。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>收割者輪詢間隔</ext:label> 
                    <ext:group id="invalidation"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="forceInvalidationMultiple" use="optional" default="3" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>如果您的要求通常不受限於回應時間限制，請指定 0 來指出階段作業管理程式在試圖使階段作業失效之前，應無限期等待直到要求完成。否則，請將這個內容設定為正整數，以延遲讓作用中階段作業失效。作用中的逾時階段作業將不會因第一個失效間隔傳遞而失效，但是將會根據此值的間隔傳遞而失效。例如，值 2 會在階段作業逾時值過期之後，使第二個失效間隔傳遞上的作用中階段作業失效。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>多重強制失效</ext:label> 
                    <ext:group id="invalidation"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cookieName" use="optional" default="JSESSIONID" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>階段作業管理 Cookie 的唯一名稱。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Cookie 名稱</ext:label> 
                    <ext:group id="cookies"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cookieDomain" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>階段作業追蹤 Cookie 的網域欄位。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Cookie 網域</ext:label> 
                    <ext:group id="cookies"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cookieMaxAge" use="optional" default="-1" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>Cookie 可以位於用戶端瀏覽器上的時間量上限。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Cookie 存在期間上限</ext:label> 
                    <ext:group id="cookies"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cookiePath" use="optional" default="/" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Cookie 會傳送至路徑中指定的 URL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Cookie 路徑</ext:label> 
                    <ext:group id="cookies"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="useContextRootAsCookiePath" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指明 Cookie 路徑等於 Web 模組的環境定義根目錄，而不是 /</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>使用環境定義根目錄作為 Cookie 路徑</ext:label> 
                    <ext:group id="cookies"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cookieSecure" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定階段作業 Cookie 是否包含安全欄位。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>將 Cookie 限制於 HTTPS 階段作業</ext:label> 
                    <ext:group id="cookies"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cookieHttpOnly" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定階段作業 Cookie 是否包含 HttpOnly 欄位。支援 HttpOnly 欄位的瀏覽器不會讓用戶端 Script 可以存取 Cookie。使用 HttpOnly 欄位將有助於防止跨網站 Script 攻擊。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>在階段作業 Cookie 中包含 HttpOnly 欄位</ext:label> 
                    <ext:group id="cookies"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="rewriteId" use="optional" default="jsessionid" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>請使用這個內容來變更與 URL 重新編寫一起使用的索引鍵。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>重新編寫 ID</ext:label> 
                    <ext:group id="url.rewriting"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="protocolSwitchRewritingEnabled" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>當 URL 需要從 HTTP 切換至 HTTPS 或從 HTTPS 切換至 HTTP 時，將階段作業 ID 新增至 URL 中。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用通訊協定切換重新編寫</ext:label> 
                    <ext:group id="url.rewriting"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="alwaysEncodeUrl" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Servlet 2.5 規格指定不將 response.encodeURL 呼叫上的 URL 編碼（如果非必要的話）。如果要在已啟用 URL 編碼時支援舊版相容性，請將這個內容設定為 true 以呼叫 encodeURL 方法。即使瀏覽器支援 Cookie，也一律會將 URL 編碼。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>一律編碼 URL</ext:label> 
                    <ext:group id="url.rewriting"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="securityIntegrationEnabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>啟用安全整合，其會導致階段作業管理機能建立使用者身分與其 HTTP 階段作業的關聯性。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>將使用者的身分與 HTTP 階段作業建立關聯</ext:label> 
                    <ext:group id="security"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="securityUserIgnoreCase" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指出即使階段作業安全身分和用戶端安全身分的大小寫不同，它們也應視為相符。例如，當這個內容設定為 true 時，階段作業安全身分 USER1 符合用戶端安全身分 User1 和 user1。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>安全使用者不區分大小寫</ext:label> 
                    <ext:group id="security"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="invalidateOnUnauthorizedSessionRequestException" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>對於回應未獲授權的要求，如果您希望階段作業管理程式讓階段作業失效，請將此內容設為 true，而不是發出 UnauthorizedSessionRequestException。當階段作業失效時，要求端可以建立新的階段作業，但是不能存取任何先前儲存的階段作業資料。這可讓單一使用者在登出之後，可繼續處理對其他應用程式的要求，而且又能保護階段作業資料。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>無效使用者要求的失效階段作業</ext:label> 
                    <ext:group id="security"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cloneId" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>叢集成員的複本 ID。在叢集內，這個 ID 必須是唯一的，才能保持階段作業親緣性。當設定時，這個名稱會改寫伺服器所產生的預設名稱。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>複本 ID</ext:label> 
                    <ext:group id="failover"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cloneSeparator" use="optional" default=":" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>用來將階段作業 ID 與階段作業 Cookie 中的複本 ID 分隔的單一字元。通常應使用預設值。在某些「無線應用程式通訊協定 (WAP)」裝置上，並不容許冒號 (:)，因此應改為使用加號 (+)。請少用不同的值。在使用這個內容來變更複本分隔字元之前，您應先瞭解在您系統上執行的其他產品之複本字元需求。您可以將任何字元指定為這個內容的值，這並不默示您指定的字元都能夠正確運作。這項事實也不默示 IBM 要負責修正使用替代字元所可能引起的任何問題。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>複本分隔字元</ext:label> 
                    <ext:group id="failover"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="idLength" use="optional" default="23" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>階段作業 ID 的長度。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>階段作業 ID 長度</ext:label> 
                    <ext:group id="advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="idReuse" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>在未配置階段作業持續存在的多重 JVM 環境中，將這個內容設定為 "true" 會讓階段作業管理程式能夠對所有使用者的要求使用相同的階段作業資訊，即使處理這些要求的 Web 應用程式是由不同的 JVM 所控管亦然。這個內容的預設值為 false。如果您要讓階段作業管理程式能夠使用瀏覽器傳送的階段作業 ID，以在未配置階段作業持續存在的環境所執行的 Web 應用程式之間保留階段作業資料，請將這個內容設定為 true。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>重複使用 ID</ext:label> 
                    <ext:group id="advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="noAdditionalInfo" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>強制移除在階段作業 ID 中不需要的資訊。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>沒有其他資訊</ext:label> 
                    <ext:group id="advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="debugCrossover" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>啟用這個選項可執行附加的檢查，以確認只有存取或參照與要求相關聯的階段作業，以及日誌訊息（如果偵測到任何不相符）。停用這個選項可跳過附加檢查。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>除錯交叉</ext:label> 
                    <ext:group id="advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>階段作業管理的其他相關內容。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>其他內容</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.generatePluginConfig"> 
        <xsd:annotation> 
            <xsd:documentation>產生外掛程式配置</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>產生外掛程式</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="httpEndpoint" minOccurs="0" maxOccurs="1" type="com.ibm.ws.http"> 
                <xsd:annotation> 
                    <xsd:documentation>指定 http 端點的 ID 以併入產生的 plugin-cfg.xml 檔。端點定義叢集中的伺服器。預設值是 'defaultHttpEndpoint'。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Http 端點</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="pluginInstallRoot" use="optional" default="." type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Web 儲存器外掛程式在檔案系統中的安裝位置</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Web 儲存器外掛程式位置</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="webserverPort" use="optional" default="80"> 
            <xsd:annotation> 
                <xsd:documentation>Web 伺服器 HTTP 埠</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Web 伺服器 HTTP 埠</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="-1"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="65535"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="webserverSecurePort" use="optional" default="443"> 
            <xsd:annotation> 
                <xsd:documentation>Web 伺服器 HTTPS 埠</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Web 伺服器 HTTPS 埠</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="-1"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="65535"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="sslKeyringLocation" use="optional" default="keyring.kdb" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>SSL 金鑰環的位置</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>SSL 金鑰環的位置</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslStashfileLocation" use="optional" default="keyring.sth" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>SSL 隱藏檔的位置</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>SSL 隱藏檔的位置</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslCertlabel" use="optional" default="LibertyCert" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>SSL 憑證標籤</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>SSL 憑證標籤</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="ipv6Preferred" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>偏好 IPv6</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>偏好 IPv6</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="httpEndpointRef" use="optional" default="defaultHttpEndpoint" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>指定 http 端點的 ID 以併入產生的 plugin-cfg.xml 檔。端點定義叢集中的伺服器。預設值是 'defaultHttpEndpoint'。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Http 端點</ext:label> 
                    <ext:reference>httpEndpoint</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="connectTimeout" use="optional" default="5s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>識別應用程式伺服器應維護 Web 伺服器連線的時間量上限。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>連線逾時值</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="serverIOTimeout" use="optional" default="900s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>識別 Web 伺服器外掛程式在傳送要求或接收應用程式伺服器的回應時，應等待的時間量上限。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>使用讀寫逾時值</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="wsServerIOTimeout" use="optional" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>識別 Web 伺服器外掛程式在傳送要求或接收應用程式伺服器的 WebSocket 回應時，應等待的時間量上限。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>將讀/寫逾時用於 WebSocket</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="wsServerIdleTimeout" use="optional" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>識別當終止閒置的 WebSocket 連線時，Web 伺服器外掛程式要等待的時間量上限。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Websocket 閒置連線逾時</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="extendedHandshake" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>若為 true，Web 伺服器外掛程式會使用延伸信號交換，來判斷應用程式伺服器是否正在執行。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>使用延伸信號交換</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="waitForContinue" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>若為 false（預設值），Web 伺服器外掛程式在傳送含有訊息內文的 HTTP 要求時，會包含 "Expect: 100-continue" 標頭。如果設為 true，Web 伺服器外掛程式在傳送每一項 HTTP 要求時，都會包含 "Expect: 100-continue" 標頭。如果 Web 伺服器和應用程式伺服器之間存在防火牆，且動輒因沒有要求內容而重試要求，請考量將此值設為 true。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>一律使用 "Expect: 100-continue" 標頭</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="logDirLocation" use="optional" type="dirLocation"> 
            <xsd:annotation> 
                <xsd:documentation>識別 http_plugin.log 檔所在的目錄。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>http_plugin.log 檔的目錄</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.webcontainer"> 
        <xsd:annotation> 
            <xsd:documentation>Web 儲存器的配置。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Web 儲存器</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="listeners" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>以逗點區隔的接聽器類別清單。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>接聽器</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="decodeUrlAsUtf8" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>利用 UTF-8 的編碼設定來進行 URL 解碼</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>利用 UTF-8 編碼來進行 URL 解碼</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="fileServingEnabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果未對應用程式明確指定此設定，會啟用檔案提供。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用檔案處理</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="disallowAllFileServing" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>停用應用程式的所有檔案提供。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.disallowAllFileServing。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>不允許所有檔案提供</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="directoryBrowsingEnabled" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>啟用應用程式的目錄瀏覽。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用目錄瀏覽</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="serveServletsByClassnameEnabled" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果未明確指定此設定，則可利用類別名稱在 Web 應用程式中存取 Servlet。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>依類別名稱提供 Servlet</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="disallowServeServletsByClassName" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>不允許在應用程式伺服器層次使用 serveServletsByClassnameEnabled。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.disallowserveservletsbyclassname。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>不允許依類別名稱提供 Servlet</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="doNotServeByClassName" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>完全不允許依類別名稱提供且以分號區隔的類別清單。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.donotservebyclassname。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>不依類別名稱提供</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="httpsIndicatorHeader" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>如果是 SSL 卸載，請設為 SSL 加速器/Proxy/負載平衡器所插入的 HTTP 標頭變數名稱。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>HTTPS 指示器標頭</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="exposeWebInfOnDispatch" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>若為 true，Servlet 便可存取 WEB-INF 目錄中的檔案。若為 false（預設值），Servlet 就無法存取 WEB-INF 目錄中的檔案。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>在分派時公開 Web 資訊</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="decodeUrlPlusSign" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>將屬於 URL 一部分的加號解碼。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.decodeurlplussign。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>解碼 URL 加號</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="channelWriteType" use="optional" default="async" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>當設為「同步」時，會同步寫入回應；否則會非同步寫入回應。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.channelwritetype。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>通道寫入類型</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="suppressHtmlRecursiveErrorOutput" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>當有應用程式配置的錯誤頁面所無法處理的遞迴錯誤時，抑制 HTML 輸出顯示異常狀況資訊。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.suppressHtmlRecursiveErrorOutput。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>抑制 HTML 回應中的遞迴異常狀況</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="fileWrapperEvents" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Web 儲存器在提供靜態檔案時會產生 SMF 和 PMI 資料。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.fileWrapperEvents。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>檔案封套事件</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="defaultTraceRequestBehavior" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>還原 HTTP TRACE 處理。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.DefaultTraceRequestBehavior。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>容許呼叫 HTTP TRACE 要求方法</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="defaultHeadRequestBehavior" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>還原 HEAD 要求不受針對 GET 方法所定義之安全限制的行為。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.DefaultHeadRequestBehavior。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>容許呼叫 HTTP HEAD 要求方法</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="tolerateSymbolicLinks" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>使 Web 儲存器能夠支援使用符號鏈結。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.TolerateSymbolicLinks。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>容許符號鏈結</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="symbolicLinksCacheSize" use="optional" default="1000" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>符號鏈結快取的起始大小。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.SymbolicLinksCacheSize。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>符號鏈結快取記憶體大小</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="enableErrorExceptionTypeFirst" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Web 儲存器更新為先搜尋及使用異常狀況類型，再搜尋及使用錯誤碼。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.enableErrorExceptionTypeFirst。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>先啟用錯誤異常狀況類型</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="enableMultiReadOfPostData" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>保留 POST 資料以供多個讀取權使用。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.enablemultireadofpostdata。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用多次讀取 POST 資料</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="copyAttributesKeySet" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Web 儲存器會將屬性清單副本的列舉傳回 Servlet，以避免 Servlet 所造成的並行存取錯誤。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.copyattributeskeyset。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>複製要求屬性清單</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="dispatcherRethrowsEr" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Web 儲存器會重新擲出錯誤，讓相關的資源處理它們。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.dispatcherRethrowser。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>重新擲出錯誤</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="ignoreSessiononStaticFileRequest" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>提高效能的方法為，防止 Web 儲存器存取涉及過濾器之靜態檔案要求的階段作業。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.IgnoreSessiononStaticFileRequest。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>忽略靜態檔案要求的階段作業</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="invokeFilterInitAtStartup" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Web 儲存器會在應用程式啟動時呼叫過濾器的 init() 方法。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.invokeFilterInitAtStartup。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟動時呼叫過濾器起始設定</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="enableJspMappingOverride" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>容許置換 JSP 對映，以便應用程式可以自行提供 JSP 內容。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.enablejspmappingoverride。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>JSP 對映置換</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="enableDefaultIsElIgnoredInTag" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>一律評估是否忽略標籤檔中的 EL 表示式。如果母項 JSP 檔有不同的 isELIgnored 設定，就會在標籤檔中重新評估設定。在完整設定檔應用程式伺服器中，對等的自訂內容是 com.ibm.ws.jsp.enabledefaultiselignoredintag。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>評估 JSP 標籤檔中的 isELIgnored 行為</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="parseUtf8PostData" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Web 儲存器會偵測非 URL 編碼的 UTF-8 POST 資料，並將它併入參數值中。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.parseutf8postdata。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>剖析 UTF-8 POST 資料</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="logServletContainerInitializerClassLoadingErrors" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>將 Servlet 儲存器類別載入錯誤記載為「警告」，而不只是在啟用除錯時記載它們。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.logservletcontainerinitializerclassloadingerrors。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>ServletContainerIntializer 錯誤警告</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowIncludeSendError" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>容許 RequestDispatch 傳送 Include 方法錯誤。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.allowincludesenderror。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>容許 Include 傳送錯誤</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="skipMetaInfResourcesProcessing" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>不要在 meta-inf 目錄中搜尋應用程式資源。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.skipmetainfresourcesprocessing。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>略過 meta-inf 資源處理</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="metaInfResourcesCacheSize" use="optional" default="20" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>設定 meta-inf 資源快取的起始大小（項目數）。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.metainfresourcescachesize.name。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Meta-inf 資源快取記憶體大小</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="xPoweredBy" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>X-Powered-By 標頭設定的替代字串。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.xpoweredby。這個內容沒有預設值。如果未設定內容，X-Powered-By 標頭的值會設為 Servlet/&lt;servlet 規格版本&gt;，如 Servlet 規格所定義。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>X-Powered-By</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="disableXPoweredBy" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>停用設定 X-Powered-By 標頭。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.disablexpoweredby。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>停用 X-Powered-By 標頭</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="deferServletLoad" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>延遲 Servlet 載入和起始設定，直到提出第一個要求為止。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>延遲 Servlet 載入</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="asyncMaxSizeTaskPool" use="optional" default="5000" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>自動清除取消的作業之前，非同步作業儲存區中的作業大小上限。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.asyncmaxsizetaskpool。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>非同步 Servlet 儲存區大小上限</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="asyncPurgeInterval" use="optional" default="30000" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>已取消的作業儲存區的每個必要清除之間所要等待的時間間隔。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.asyncpurgeinterval。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>非同步 Servlet 清除間隔</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="asyncTimeoutDefault" use="optional" default="30000" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>未明確指定逾時值時所用的非同步 Servlet 逾時值。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.asynctimeoutdefault。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>預設非同步逾時值</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="asyncTimerThreads" use="optional" default="2" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>要用於非同步 Servlet 逾時處理的執行緒數目上限。在完整應用程式伺服器設定檔中的對等自訂內容是 com.ibm.ws.webcontainer.asynctimerthreads。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>非同步計時器執行緒</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="setContentLengthOnClose" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>當應用程式明確關閉回應時，切換成設定內容長度。預設值是 true；不過，如果應用程式的回應含有雙位元組字元，請將此值設為 false。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>關閉時設定內容長度</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="emptyServletMappings" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>當未新增 Servlet 對映時，如果您想傳回空集合，而不是傳回空值，請切換這個選項。預設值是 false。在完整應用程式伺服器設定檔中，其對等自訂內容是 com.ibm.ws.webcontainer.emptyservletmappings。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>傳回空白 Servlet 對映</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="deferServletRequestListenerDestroyOnError" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>當處理要求期間發生錯誤時，如果您想延遲毀損 ServletRequestListener，請切換這個選項。預設值是 false。在完整應用程式伺服器設定檔中，其對等自訂內容是 com.ibm.ws.webcontainer.deferServletRequestListenerDestroyOnError。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>延遲毀損 Servlet 要求接聽器</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowExpressionFactoryPerApp" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>切換以載入應用程式設定的 ExpressionFactory。如果您要使用的自訂 EL 實作（例如 JUEL）需要設定自己的 ExpressionFactory，請啟用此自訂內容。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>依每個應用程式容許 ExpressionFactory</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="ignoreSemiColonOnRedirectToWelcomePage" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>切換此項，以便在重新導向至歡迎使用頁面時，忽略尾端的分號。預設值是 false。在完整應用程式伺服器設定檔中，其對等自訂內容是 com.ibm.ws.webcontainer.ignoreSemiColonOnRedirectToWelcomePage。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>在重新導向至歡迎使用頁面時忽略分號</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="useSemiColonAsDelimiterInURI" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>切換此項，以便在要求 URI 中使用分號作為定界字元。預設值是 false。在完整應用程式伺服器設定檔中，其對等自訂內容是 com.ibm.ws.webcontainer.useSemiColonAsDelimiterInURI。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>在 URI 中使用分號作為定界字元</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.session.db"> 
        <xsd:annotation> 
            <xsd:documentation>控制 HTTP 階段作業如何持續保存到資料庫。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>HTTP 階段作業資料庫</ext:label> 
                <ext:extraProperties/> 
                <ext:groupDecl id="performance" label="效能">變更這些設定可加強效能。</ext:groupDecl> 
                <ext:groupDecl id="advanced.performance" label="進階效能">變更這些設定可加強效能，但是預設值適用於大部分的環境。</ext:groupDecl> 
                <ext:groupDecl id="db2" label="僅限 DB2">這些選用設定僅適用於 DB2 資料庫。</ext:groupDecl> 
                <ext:groupDecl id="oracle" label="僅限 Oracle">這些選用設定僅適用於 Oracle 資料庫。</ext:groupDecl> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="dataSourceRef" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>階段作業管理程式應用來持續保存 HTTP 階段作業資料的資料來源 ID。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>資料來源</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="tableName" use="optional" default="sessions" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>資料庫表格名稱。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>表格名稱</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="useMultiRowSchema" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>當啟用時，每一個階段作業資料屬性都會置於資料庫中的個別列中，以儲存每一個階段作業更大量的資料。當階段作業屬性極大，且需要稍微變更階段作業屬性時，這種配置可產生較佳的效能。當停用時，所有的階段作業資料屬性會置於每一個階段作業的同一列中。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>使用多列綱目</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="skipIndexCreation" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>將這個內容設定為 "true"，在伺服器啟動時，會停用建立索引的功能。只有在您想要針對階段作業持續性來手動建立自己的資料庫索引時，才應使用這個自訂內容。不過，建議您讓階段作業管理程式建立資料庫索引。在啟用這個內容之前，請確定您的階段作業資料庫有正確的索引。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>跳過建立索引</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="writeFrequency" use="optional" default="END_OF_SERVLET_SERVICE"> 
            <xsd:annotation> 
                <xsd:documentation>指定階段作業資料何時寫入持續儲存庫。依預設，階段作業資料會在 Servlet 完成執行之後寫入持續儲存庫。在某些環境中，變更此值可以增進效能。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>寫入頻率</ext:label> 
                    <ext:group id="performance"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="END_OF_SERVLET_SERVICE"> 
                                <xsd:annotation> 
                                    <xsd:documentation>階段作業資料會在 Servlet 完成執行之後寫入持續儲存庫。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="MANUAL_UPDATE"> 
                                <xsd:annotation> 
                                    <xsd:documentation>必須在 IBMSession 物件上進行程式化同步，才能將階段作業資料寫入持續儲存庫。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="TIME_BASED_WRITE"> 
                                <xsd:annotation> 
                                    <xsd:documentation>階段作業資料會根據指定的寫入間隔值寫入持續儲存庫。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="writeInterval" use="optional" default="2m" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>將階段作業資料寫入持續儲存庫之前應經過的秒數。預設值是 120 秒。只有在已啟用時間型寫入頻率時，才會使用這個值。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>寫入間隔</ext:label> 
                    <ext:group id="performance"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="writeContents" use="optional" default="ONLY_UPDATED_ATTRIBUTES"> 
            <xsd:annotation> 
                <xsd:documentation>指定多少階段作業資料應寫入持續儲存庫。依預設，只會寫入已更新的屬性，但是可以改為寫入所有的屬性（無論它們是否已變更）。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>寫入內容</ext:label> 
                    <ext:group id="performance"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="ONLY_UPDATED_ATTRIBUTES"> 
                                <xsd:annotation> 
                                    <xsd:documentation>只將更新的屬性寫入持續儲存庫中。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="ALL_SESSION_ATTRIBUTES"> 
                                <xsd:annotation> 
                                    <xsd:documentation>所有屬性都寫入持續儲存庫中。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="scheduleInvalidation" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>啟用這個選項可減少保持 HTTP 階段作業作用中所需的資料庫更新項目數目。當應用程式伺服器中的活動最少時，請指定一天兩小時。當這個選項停用時，失效程式程序會每隔幾分鐘執行來移除失效的 HTTP 階段作業。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>排定失效時程表</ext:label> 
                    <ext:group id="advanced.performance"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="scheduleInvalidationFirstHour" use="optional" default="0" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>指出會從持續儲存庫清除失效的階段作業期間的第一個小時。請將這個值指定為介於 0 與 23 之間的整數。這個值只在啟用了排程失效時才有效。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>當天的第一個小時</ext:label> 
                    <ext:group id="advanced.performance"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="scheduleInvalidationSecondHour" use="optional" default="0" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>指出會從持續儲存庫清除失效的階段作業期間的第二個小時。請將這個值指定為介於 0 與 23 之間的整數。這個值只在啟用了排程失效時才有效。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>當天的第二個小時</ext:label> 
                    <ext:group id="advanced.performance"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="noAffinitySwitchBack" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>將這個內容設定為 "true" 時，即使原始成員回復，仍可以保持與新成員的親緣性。當叢集成員失敗時，其要求會遞送到不同的叢集成員，並在這些成員中啟動階段作業。因此，會保持與新成員的階段作業親緣性，並且當失敗的叢集成員回復時，在原始叢集成員中所建立的階段作業要求會遞送回失敗的叢集成員。接受的值為 true 或 false，預設值為 false。當您將配送的階段作業配置為時間型寫入時，請將這個內容設定為 true。請注意，當未啟用配送的階段作業時，這個內容對於行為並沒有任何影響。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>無親緣性切回</ext:label> 
                    <ext:group id="advanced.performance"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="useInvalidatedId" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果具備送入 ID 的階段作業最近失效，將這個內容設定為 "true" 可重複使用這個 ID。這可效能最佳化，因為它防止檢查持續儲存庫。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>使用失效的 ID</ext:label> 
                    <ext:group id="advanced.performance"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="onlyCheckInCacheDuringPreInvoke" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>值 true 表示只有在要求取得階段作業時，才應更新階段作業的前次存取時間。值 false 表示在每個要求之後，都應立即更新階段作業的前次存取時間。在某些環境中，變更此值可以增進效能。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>僅在前置呼叫期間移入快取</ext:label> 
                    <ext:group id="advanced.performance"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="optimizeCacheIdIncrements" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果使用者的瀏覽器階段作業在多個 Web 應用程式之間來回移動，則在從持續儲存庫重新整理 Web 模組的記憶體內階段作業時，您可能會看到額外的持續儲存庫活動。因此，快取 ID 會持續增大，記憶體內階段作業屬性會被持續副本的所改寫。如果您想要防止快取 ID 持續增大，請將這個內容設定為 true。值 true 表示階段作業管理程式應評量 Web 模組的記憶體內階段作業是否比持續儲存庫中的副本來得舊。如果配置為叢集，請確保每一個叢集成員的系統時間盡可能相同。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>最佳化快取 ID 增量</ext:label> 
                    <ext:group id="advanced.performance"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="db2RowSize" use="optional" default="4KB"> 
            <xsd:annotation> 
                <xsd:documentation>配置給階段作業表格的表格空間頁面大小（如果使用 DB2 資料庫的話）。在某些環境中，增加此值可以增進資料庫效能。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>DB2 列大小</ext:label> 
                    <ext:group id="db2"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="4KB"> 
                                <xsd:annotation> 
                                    <xsd:documentation>使用預設表格空間頁面大小 4 KB。您不需要建立 DB2 緩衝池或表格空間，而且不需要指定表格空間名稱。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="8KB"> 
                                <xsd:annotation> 
                                    <xsd:documentation>使用表格空間頁面大小 8 KB。您必須另外建立 DB2 緩衝池和表格空間，並指定 8KB 作為兩者的頁面大小。您也必須指定您所建立之表格空間的名稱。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="16KB"> 
                                <xsd:annotation> 
                                    <xsd:documentation>使用表格空間頁面大小 16 KB。您必須另外建立 DB2 緩衝池和表格空間，並指定 16KB 作為兩者的頁面大小。您也必須指定您所建立之表格空間的名稱。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="32KB"> 
                                <xsd:annotation> 
                                    <xsd:documentation>使用表格空間頁面大小 32 KB。您必須另外建立 DB2 緩衝池和表格空間，並指定 32KB 作為兩者的頁面大小。您也必須指定您所建立之表格空間的名稱。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="tableSpaceName" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>要用於階段作業表格的表格空間。只有在 DB2 列大小大於 4KB 時，才需要這個值。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>表格空間名稱</ext:label> 
                    <ext:group id="db2"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="usingCustomSchemaName" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果您將 DB2 用於階段作業持續性，且已在資料來源中設定 currentSchema 內容，請將這個內容設為 "true"。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>使用自訂綱目名稱</ext:label> 
                    <ext:group id="db2"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="useOracleBlob" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>將這個內容設定為 "true" 時，可以使用「二進位大型物件 (Binary Large Object, BLOB)」資料類型，針對媒介直欄來建立資料庫表格。使用 Oracle 資料庫時，這個值可增加持續性階段作業的效能。由於 Oracle 的限制，對於超過 4000 個位元組的資料，BLOB 支援需要使用 Oracle Call Interface (OCI) 資料庫驅動程式。您也必須確保在伺服器重新啟動之前建立新的階段作業表格，方法為捨棄舊的階段作業表格，或是將資料來源定義變更為參照不包含階段作業表格的資料庫。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>媒體直欄使用「二進位大型物件 (BLOB)」</ext:label> 
                    <ext:group id="oracle"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>階段作業持續性的其他相關內容。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>其他內容</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.ssl.default"> 
        <xsd:annotation> 
            <xsd:documentation>SSL 服務的預設儲存庫。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>SSL 預設儲存庫</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="sslRef" use="optional" default="defaultSSLConfig" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>將作為預設值的 SSL 儲存庫名稱。如果都沒有指定，會使用預設 SSL 儲存庫，名稱是 defaultSSLConfig。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>預設 SSL 儲存庫</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.ssl.repertoire"> 
        <xsd:annotation> 
            <xsd:documentation>具備 ID、已定義的金鑰儲存庫及選用的信任儲存庫之 SSL 儲存庫。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>SSL 儲存庫</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="keyStoreRef" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>包含 SSL 儲存庫之金鑰項目的金鑰儲存庫。這個屬性是必要的。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>金鑰儲存庫</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="trustStoreRef" use="optional" default="${keyStoreRef}" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>包含 SSL 儲存庫用於簽署確認之信任憑證項目的金鑰儲存庫。這個屬性是選用的。如果未指定，則會對金鑰及信任的憑證項目都使用相同的金鑰儲存庫。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>信任儲存庫</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslProtocol" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>SSL 信號交換通訊協定。通訊協定值可在基礎 JRE 的 Java Secure Socket Extension (JSSE) 提供者的說明文件中找到。當使用 IBM JRE 時，預設值是 SSL_TLS，當使用 Oracle JRE 時，預設值是 SSL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>SSL 通訊協定</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="clientAuthentication" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否啟用用戶端鑑別。如果設為 true，則需要用戶端鑑別，且用戶端必須提供伺服器信任的憑證。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用戶端鑑別</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="clientAuthenticationSupported" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否支援用戶端鑑別。如果設為 true，則用戶端鑑別支援表示一旦用戶端出示憑證，伺服器會檢查是否信任該用戶端。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>支援用戶端鑑別</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="securityLevel" use="optional" default="HIGH"> 
            <xsd:annotation> 
                <xsd:documentation>指定 SSL 信號交換所用的密碼組合群組。「高」是 3DES 和 128 位元（及以上）密碼，「中」是 DES 和 40 位元密碼，「低」是沒有加密的密碼。如果使用 enabledCiphers 屬性，則會忽略 securityLevel 清單。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>安全層次</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="HIGH"> 
                                <xsd:annotation> 
                                    <xsd:documentation>密碼組合 3DES 和 128 位元（及以上）</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="MEDIUM"> 
                                <xsd:annotation> 
                                    <xsd:documentation>%repertoire.MEDIUM</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="LOW"> 
                                <xsd:annotation> 
                                    <xsd:documentation>%repertoire.LOW</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="CUSTOM"> 
                                <xsd:annotation> 
                                    <xsd:documentation>%repertoire.CUSTOM</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="clientKeyAlias" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定金鑰儲存庫中的憑證別名，該憑證將作為金鑰，並傳送給已啟用用戶端鑑別的伺服器。只有在金鑰儲存庫有多個金鑰項目時，才需要這個屬性。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用戶端金鑰別名</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="serverKeyAlias" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定金鑰儲存庫中要作為伺服器金鑰之憑證的別名。只有在金鑰儲存庫有多個金鑰項目時，才需要這個屬性。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>伺服器金鑰別名</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="enabledCiphers" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定自訂的密碼清單。在清單中，各密碼之間請空一格。支援的密碼取決於所使用的基礎 JRE。請檢查 JRE，取得有效的密碼。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>已啟用密碼</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.ssl.repertoire-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.ssl.repertoire"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.ssl.keystore"> 
        <xsd:annotation> 
            <xsd:documentation>用於 SSL 加密的安全憑證之儲存庫。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>金鑰儲存庫</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="keyEntry" minOccurs="0" maxOccurs="unbounded" type="com.ibm.ws.ssl.keystore.config.keyEntry-factory"> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="password" use="required" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>用來載入金鑰儲存庫檔的密碼。其值可以用明碼或編碼形式儲存。請使用 securityUtility 工具來將密碼編碼。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>密碼</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="location" use="optional" default="${server.output.dir}/resources/security/key.jks" type="fileLocation"> 
            <xsd:annotation> 
                <xsd:documentation>金鑰儲存庫檔的絕對路徑或相對路徑。如果提供相對路徑，伺服器將會嘗試在 ${server.config.dir}/resources/security 目錄中找出此檔案。對檔案型金鑰儲存庫使用金鑰儲存庫檔；對 SAF 金鑰環使用金鑰環名稱，或是對硬體加密裝置使用裝置配置檔。在 SSL 最小配置中，檔案的位置是假設為 ${server.config.dir}/resources/security/key.jks。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>位置</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="type" use="optional" default="jks" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>目標 SDK 支援的金鑰儲存庫類型。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>類型</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="fileBased" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果金鑰儲存庫是檔案型，請指定 true；如果金鑰儲存庫是 SAF 金鑰環或硬體金鑰儲存庫類型，請指定 false。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>檔案型金鑰儲存庫</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="readOnly" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果伺服器只為了讀取而使用金鑰儲存庫，請指定 true；如果伺服器會對金鑰儲存庫執行寫入作業，請指定 false。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>唯讀金鑰儲存庫</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="pollingRate" use="optional" default="500ms" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>伺服器檢查金鑰儲存庫檔更新項目的頻率。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m)、秒 (s) 或毫秒 (ms)。例如，指定 500 毫秒為 500ms。您可以在單一項目中包括多個值。例如，1s500ms 相等於 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>金鑰儲存庫檔輪詢率</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="updateTrigger" use="optional" default="mbean"> 
            <xsd:annotation> 
                <xsd:documentation>金鑰儲存庫檔案更新方法或觸發程式。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>金鑰儲存庫檔案更新觸發程式</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="polled"> 
                                <xsd:annotation> 
                                    <xsd:documentation>伺服器會依輪詢間隔來掃描金鑰儲存庫檔案的變更，如果可偵測到金鑰儲存庫檔案的變更，就會進行更新。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="mbean"> 
                                <xsd:annotation> 
                                    <xsd:documentation>只在 FileNotificationMbean 提示時，伺服器才更新金鑰儲存庫。外部程式（例如：整合開發環境或管理應用程式）通常會呼叫 FileNotificationMBean。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="disabled"> 
                                <xsd:annotation> 
                                    <xsd:documentation>停用所有更新監視。當伺服器正在執行時，不會套用金鑰儲存庫檔案的變更。</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.ssl.keystore-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.ssl.keystore"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.ssl.keystore.config.keyEntry"> 
        <xsd:annotation> 
            <xsd:documentation>金鑰儲存庫中指定之私密金鑰項目的相關資訊。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>金鑰項目資訊</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="name" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>金鑰儲存庫中之私密金鑰項目的名稱。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>金鑰項目名稱</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="keyPassword" use="required" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>金鑰儲存庫中之私密金鑰項目的密碼。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>金鑰項目密碼</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.ssl.keystore.config.keyEntry-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.ssl.keystore.config.keyEntry"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.sslchannel.options"> 
        <xsd:annotation> 
            <xsd:documentation>用於傳輸的 SSL 通訊協定配置。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>SSL 選項</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="sessionTimeout" use="optional" default="1d" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>在 Socket 上等待讀取或寫入要求完成的時間量。特定通訊協定專用的逾時值會置換此值。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>階段作業逾時值</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="suppressHandshakeErrors" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>停用 SSL 信號交換記載錯誤。在正常作業期間可能會發生 SSL 信號交換錯誤，不過當 SSL 的行為並非所預期時，這些訊息可能會非常有用。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>暫停 SSL 信號交換錯誤</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslRef" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>預設 SSL 配置儲存庫。預設值是 defaultSSLSettings。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>預設 SSL 儲存庫</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.sslchannel.options-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.sslchannel.options"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.transaction"> 
        <xsd:annotation> 
            <xsd:documentation>交易管理程式服務的配置內容</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>交易管理程式</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="dataSource" minOccurs="0" maxOccurs="1" type="com.ibm.ws.jdbc.dataSource"> 
                <xsd:annotation> 
                    <xsd:documentation>這個是一個選用的內容。依預設，交易服務會將它的回復日誌儲存在檔案中。作為替代方案，日誌也可能儲存在 RDBMS 中。方式是設定這個內容來定義將儲存交易日誌的非交易式資料來源。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>非交易式資料來源</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="recoverOnStartup" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指明在伺服器啟動時，伺服器是否要開始交易回復。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟動時回復</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="waitForRecovery" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指明伺服器是否要等待交易回復完成，才接受新的交易式工作。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>等待回復</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="acceptHeuristicHazard" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指明這個伺服器上的所有應用程式，是否接受內含一段式資源的兩段式交易中可能發生的探索性風險。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>接受探索性風險</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="clientInactivityTimeout" use="optional" default="60s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>遠端用戶端發出的交易要求之間的持續時間上限。用戶端閒置超出這個逾時值的任何期間，都會在這個應用程式伺服器中造成交易的回復。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用戶端閒置逾時</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="heuristicRetryInterval" use="optional" default="60s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>在資源管理程式或遠端夥伴擲出暫時性異常狀況之後，在重試確定或回復之類的完成信號之前，應用程式伺服器等待的時間量。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>探索重試等待</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="heuristicRetryWait" use="optional" default="5" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>應用程式伺服器重試完成信號（如確定或回復）的次數。在資源管理程式或遠端夥伴擲出暫時性異常狀況之後，便會重試。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>探索重試限制</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="propogatedOrBMTTranLifetimeTimeout" use="optional" default="0" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>在這個伺服器中所執行交易的交易逾時上限。這個值應該大於或等於指定的總交易逾時值。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>交易逾時值上限</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="totalTranLifetimeTimeout" use="optional" default="120s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>容許在這個伺服器中啟動之交易的預設完成時間上限。未在這個逾時值之前完成的任何這類交易都會回復。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>交易生命週期逾時總計</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="transactionLogDirectory" use="optional" default="${server.output.dir}/tranlog/" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>在這個伺服器中，交易服務用來儲存回復所用日誌檔的目錄。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>交易日誌目錄</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="transactionLogSize" use="optional" default="1024" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>指定交易日誌檔的大小（以 KB 為單位）。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>交易日誌大小</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="recoveryIdentity" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>用於交易同層級回復之這部伺服器的唯一身分。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用於交易同層級回復的伺服器身分</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="recoveryGroup" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>此伺服器所屬之回復群組的名稱。回復群組成員可回復群組中其他伺服器的交易日誌。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>回復群組的名稱</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="enableLoggingForHeuristicReporting" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指明對於同時涉及一段式確定資源和兩段式確定資源的交易所發出的「即將確定一段式資源」事件，應用程式伺服器是否要加以記載。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用探索報告的記載功能</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="transactionLogDBTableSuffix" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>當回復日誌儲存在 RDBMS 表格時，這個內容容許事後使用字串來決定表格名稱，讓這部伺服器將它視為唯一的。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>交易日誌資料庫表格的字尾</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="timeoutGracePeriodEnabled" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指明執行交易之服務者區域的交易逾時與異常結束之間是否有延遲。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>啟用交易逾時寬限期</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="lpsHeuristicCompletion" use="optional" default="ROLLBACK"> 
            <xsd:annotation> 
                <xsd:documentation>指明用來完成含探索輸出之交易的方向；可能由應用程式伺服器來確定或回復交易，或是取決於管理者的手動完成。容許的值為：「確定」、「回復」和「手動」</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>探索完成方向</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="ROLLBACK"> 
                                <xsd:annotation> 
                                    <xsd:documentation>回復</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="COMMIT"> 
                                <xsd:annotation> 
                                    <xsd:documentation>確定</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="MANUAL"> 
                                <xsd:annotation> 
                                    <xsd:documentation>手動</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="defaultMaxShutdownDelay" use="optional" default="2s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>預設關閉延遲時間上限. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>預設關閉延遲時間上限</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="dataSourceRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>這個是一個選用的內容。依預設，交易服務會將它的回復日誌儲存在檔案中。作為替代方案，日誌也可能儲存在 RDBMS 中。方式是設定這個內容來定義將儲存交易日誌的非交易式資料來源。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>非交易式資料來源參照</ext:label> 
                    <ext:reference>dataSource</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.jca.jmsQueue.properties.wasJms.javax.jms.Queue-com.ibm.ws.sib.api.jms.impl.JmsQueueImpl"> 
        <xsd:annotation> 
            <xsd:documentation>這個 JMS 佇列所指派的佇列名稱。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>內嵌傳訊</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="deliveryMode" use="optional" default="Application"> 
            <xsd:annotation> 
                <xsd:documentation>傳送給這個目的地的訊息遞送模式。這用來控制這個目的地的訊息持續性。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>遞送模式</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="Application"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Application</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="Persistent"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Persistent</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="NonPersistent"> 
                                <xsd:annotation> 
                                    <xsd:documentation>NonPersistent</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="priority" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>傳送給這個目的地的訊息的相對優先順序，範圍是 0 至 9，0 是最低優先順序，9 是最高優先順序。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>優先順序</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="9"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="queueName" use="optional" default="Default.Queue" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>相關佇列的名稱</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>佇列名稱</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="readAhead" use="optional" default="AsConnection"> 
            <xsd:annotation> 
                <xsd:documentation>先讀是一種事先將訊息指派給消費者的最佳化。如此可讓處理消費者要求的速度會更快。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>先讀</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="AsConnection"> 
                                <xsd:annotation> 
                                    <xsd:documentation>AsConnection</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="AlwaysOn"> 
                                <xsd:annotation> 
                                    <xsd:documentation>AlwaysOn</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="AlwaysOff"> 
                                <xsd:annotation> 
                                    <xsd:documentation>AlwaysOff</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
