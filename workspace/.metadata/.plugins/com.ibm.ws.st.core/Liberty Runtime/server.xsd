    <xsd:complexType name="com.ibm.ws.cache"> 
        <xsd:annotation> 
            <xsd:documentation>本端快取的分散式對映配置。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>分散式對映</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="library" type="com.ibm.ws.classloading.sharedlibrary"> 
                <xsd:annotation> 
                    <xsd:documentation>指定共用程式庫的參照。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>共用程式庫</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="cacheGroup" type="com.ibm.ws.cache.cacheGroup"> 
            </xsd:element> 
            <xsd:element name="diskCache" type="com.ibm.ws.cache.disk"> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="jndiName" use="optional" default="${id}" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>快取實例的 JNDI 名稱。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>JNDI 名稱</ext:label> 
                    <ext:unique>jndiName</ext:unique> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="memorySizeInEntries" use="optional" default="2000"> 
            <xsd:annotation> 
                <xsd:documentation>指定一個正整數來定義快取所能保留的項目數上限。值通常數以千計。最小值是 100，沒有設定最大值。預設值為 2000。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>記憶體快取大小</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="memorySizeInMB" use="optional" default="-1"> 
            <xsd:annotation> 
                <xsd:documentation>指定記憶體快取大小上限值 (MB)。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>記憶體快取大小 (MB)</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="-1"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="highThreshold" use="optional" default="-1"> 
            <xsd:annotation> 
                <xsd:documentation>指定何時啟動記憶體快取收回原則。這個臨界值是用記憶體快取大小 (MB) 的百分比來表示。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>高臨界值</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="-1"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="100"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="lowThreshold" use="optional" default="-1"> 
            <xsd:annotation> 
                <xsd:documentation>指定何時結束記憶體快取收回原則。這個臨界值是用記憶體快取大小 (MB) 的百分比來表示。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>低臨界值</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="-1"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="100"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="cacheProviderName" use="optional" default="default" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定替代快取提供者的名稱。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>快取提供者</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="libraryRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>指定共用程式庫的參照。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>共用程式庫參照</ext:label> 
                    <ext:reference>library</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
